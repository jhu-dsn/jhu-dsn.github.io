a:7:{s:5:"title";s:31:"Detailed Project Implementation";s:11:"description";a:2:{s:8:"abstract";s:400:"Each access point in the SMesh network will serve as a regular wireless access point(providing access to the Internet) and will in addition provide the push-to-talk service. The PTT service is provided by a special daemon running in every SMesh access point. This daemon is invoked every time a PTT client sends out a PTT message. A PTT client is any wireless device that runs the PTT client program.";s:15:"tableofcontents";a:3:{i:0;a:4:{s:3:"hid";s:31:"detailed_project_implementation";s:5:"title";s:31:"Detailed Project Implementation";s:4:"type";s:2:"ul";s:5:"level";i:1;}i:1;a:4:{s:3:"hid";s:47:"push-to-talk_infrastructure_in_the_mesh_network";s:5:"title";s:47:"Push-To-Talk Infrastructure in the mesh network";s:4:"type";s:2:"ul";s:5:"level";i:2;}i:2;a:4:{s:3:"hid";s:12:"the_protocol";s:5:"title";s:12:"The Protocol";s:4:"type";s:2:"ul";s:5:"level";i:2;}}}s:8:"relation";a:2:{s:7:"haspart";a:0:{}s:10:"references";a:0:{}}s:4:"date";a:3:{s:5:"valid";a:0:{}s:7:"created";i:1177284682;s:8:"modified";i:1177345877;}s:7:"creator";s:15:"Pratibha Mallya";s:11:"last_change";a:7:{s:4:"date";i:1177345877;s:2:"ip";s:15:"128.220.223.160";s:4:"type";s:1:"E";s:2:"id";s:4:"here";s:4:"user";s:7:"pmallya";s:3:"sum";s:0:"";s:5:"extra";s:0:"";}s:11:"contributor";a:2:{i:0;N;s:7:"pmallya";s:15:"Pratibha Mallya";}}